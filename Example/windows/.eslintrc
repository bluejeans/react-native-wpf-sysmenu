{
  "env": {
    "es6": true
  },

  "globals": {
  },

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "modules": true,
      "jsx": true
    },
    "sourceType": "module"
  },

  "rules": {
    "complexity": ["error", { "max": 15 }]
  }
}
